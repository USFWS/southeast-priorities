{
  "name": "priorities",
  "version": "1.0.0",
  "description": "An interactive website outlining the USFWS Southeast Region's Priorities",
  "main": "index.js",
  "watch": {
    "hugo:dev": {
      "patterns": [
        "site/**/*"
      ],
      "extensions": "html,md,ace,css,js"
    },
    "svgo": "src/images/svg/**/*"
  },
  "scripts": {
    "watch": "npm-watch",
    "presvgo": "rimraf site/static/images/svg",
    "svgo": "svgo -f src/images/svg -o site/static/images/svg",
    "prehero": "rimraf site/static/images/hero",
    "hero": "node build/resize-hero",
    "test": "echo \"Error: no test specified\" && exit 1",
    "mkdist": "node build/create-dist",
    "hugo:dev": "node build/hugo -- /",
    "hugo:stage": "node build/hugo -- https://intranet.fws.gov/region4/priorities/",
    "hugo:pro": "node build/hugo -- http://www.fws.gov/southeast/regional-priorities/",
    "watch:sass": "node-sass -w src/scss/styles.scss site/static/css/styles.css",
    "build:sass": "node-sass --output-style compressed src/scss/styles.scss | postcss -u autoprefixer -o site/static/css/styles.css",
    "serve": "browser-sync start --server dist --files 'dist/**/*'",
    "publish:demo": "npm run build && gh-pages -d dist",
    "prebuild": "npm run mkdist",
    "build": "npm run build:sass && npm run hugo:pro",
    "prestart": "npm run mkdist",
    "start": "npm run watch:sass & npm run hugo:dev & npm run watch & npm run serve"
  },
  "dependencies": {
    "d3": "^3.5.16",
    "scrollmagic": "^2.0.5",
    "scrollreveal": "^3.1.1",
    "underscore.string": "^3.3.4",
    "zenscroll": "^3.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "browser-sync": "^2.11.1",
    "gh-pages": "^0.11.0",
    "mkdirp": "^0.5.1",
    "node-sass": "^3.4.2",
    "postcss-cli": "^2.5.1",
    "rimraf": "^2.5.2",
    "sharp": "^0.13.1",
    "svgo": "^0.6.3"
  },
  "keywords": [],
  "author": "Roy Hewitt, @r0yhewitt",
  "license": "SAX-PD"
}
